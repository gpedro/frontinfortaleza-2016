doctype html
html.no-js
	head
		meta(charset='utf-8')
		title Google I/O Extended Fortaleza - 2016
		meta(name='MobileOptimized', content='320')
		meta(name='HandheldFriendly', content='True')
		meta(name='keywords', content='io2016, ioextended')
		meta(name='apple-mobile-web-app-capable', content='yes')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		meta(name='apple-mobile-web-app-status-bar-style', content='black-translucent')
		
		//- FACEBOOK META TAGS
		meta(property='og:type', content='website')
		meta(property='og:image:type', content='image/png')
		meta(itemprop='name', content='#{event.name}')
		meta(property='og:title', content='#{event.name}')
		meta(property="og:url" content="#{event.metas.url}")
		meta(property='og:site_name', content='#{event.name}')
		meta(property='og:image', content='#{event.metas.image_url}')
		meta(itemprop='image', content='#{event.metas.image_url}')
		meta(property='og:description', content='#{event.metas.description}')
		meta(property='og:image:width', content='800')
		meta(property='og:image:height', content='600')


		
		// build:css styles/main.css
		link(rel='stylesheet' href='styles/main.css')
